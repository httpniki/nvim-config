{
   "languageserver": {
      "lua": {
         "command": "lua-lsp",
         "filetypes": [
            "lua"
         ]
      }
   },
   "java.configuration.runtimes": [
      {
         "name": "JavaSE-17",
         "path": "/usr/lib/jvm/jdk-17-oracle-x64/bin/java",
         "default": true
      }
   ],
   "eslint.enable": true,
   "eslint.run": "onType",
   "eslint.format.enable": true,
   "eslint.codeAction.showDocumentation": {
      "enable": true
   },
   "diagnostic.checkCurrentLine": true,
   "diagnostic.enable": true,
   "diagnostic.errorSign": ">>",
   "diagnostic.virtualText": true,
   "diagnostic.virtualTextCurrentLineOnly": false,
   "codeLens.enable": true,
   "colors.enable": true,
   "diagnostic.virtualTextLevel": "error",
   "eslint.probe": [
      "javascript",
      "javascriptreact",
      "typescript",
      "typescriptreact",
      "html",
      "vue",
      "markdown"
   ],
   "diagnostic.floatConfig": {
      "border": false,
      "rounded": false,
      "borderhighlight": "CocFloating",
      "shadow": false,
      "winblend": 0
   },
   "suggest.floatConfig": {
      "border": false,
   },
   "typescript.showDeprecated": false,
   "suggest.insertMode": "insert",
}
